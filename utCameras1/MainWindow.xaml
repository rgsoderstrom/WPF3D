<Window x:Class="utCameras1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:utCameras1"
        mc:Ignorable="d"
        Title="MainWindow" Height="850" Width="800" Loaded="Window_Loaded">
    <DockPanel>
        <Border BorderBrush="Green" BorderThickness="2" DockPanel.Dock="Left">
            <StackPanel>
                <Button Name="Button1" Margin="10, 10, 10, 0" FontSize="16" Click="Button1_Click">Position</Button>
                <Button Name="Button2" Margin="10, 10, 10, 0" FontSize="16" Click="Button2_Click">CenterOn</Button>
                <Button Name="Button3" Margin="10, 10, 10, 0" FontSize="16" Click="Button3_Click">FOV</Button>
                <Button Name="Button4" Margin="10, 10, 10, 0" FontSize="16" Click="Button4_Click">Theta and Phi</Button>
                <Button Name="Button5" Margin="10, 10, 10, 0" FontSize="16" Click="Button5_Click">Rho</Button>
            </StackPanel>
        </Border>

        <Viewport3D Name="view"/>
    </DockPanel>
</Window>
